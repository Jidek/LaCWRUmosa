<!-- The following partial is being organized into a table to allow for CSSing per row. -->
<%=content_tag :table do%>
  <%@check_ins.each do |check_in|%>
    <%=content_tag :tr do%>
      <%=content_tag :td do%>
        <%=check_in.time.strftime("#{check_in.user.name} checked in to #{check_in.location.description} at %l:%M%p on %m/%d/%Y")%>
      <%end%>
    <%end%>
  <%end%>
<%end%>
