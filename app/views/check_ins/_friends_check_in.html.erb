<!-- The following partial is being organized into a table to allow for CSSing per row. -->
<%=content_tag :table do%>
  <%@check_ins.each do |check_in|%>
    <%=content_tag :tr do%>
      <%=content_tag :td do%>
        <%="#{check_in.user.name} checked in at #{check_in.location.description} at #{check_in.time}"%>
      <%end%>
    <%end%>
  <%end%>
<%end%>
