<%=content_tag :h1, "Edit Event"%>

<%=form_for(@event) do |form| %>
  <%=content_tag :table do%>
    <%=content_tag :tr do%>
      <%=content_tag :td do%>
        <%=render partial: 'form', locals:{form:form}%>
      <%end%>
    <%end%>
    <%=content_tag :tr do%>
      <%=content_tag :td do%>
        <%=submit_tag "Update"%>
      <%end%>
    <%end%>
  <%end%>
<%end%>
