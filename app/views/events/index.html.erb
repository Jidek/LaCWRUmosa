<%=content_tag :table do%>
  <%=content_tag :tr do %>
    <%=content_tag :th, "time"%>
    <%=content_tag :th, "Location"%>
  <%end%>
  <%@events.each do |event|%>
    <%=content_tag :tr do%>
      <%=content_tag :td, link_to(event.time, event_path(event.id))%>
      <%=content_tag :td, event.location_id%> <!-- TODO link this to locations, once locations exist -->
    <%end%>
  <%end%>
<%end%>
