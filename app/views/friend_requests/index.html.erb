<%=content_tag :table do%>
  <%@FriendRequests.each do |request|%>
    <%=content_tag :tr do%>
      <%=content_tag :td, link_to(request.from.name, user_path(request.from))%>
      <%=content_tag :td, link_to("Accept", accept_friend_request_path({id:0, from_user_id: request.from_user_id, to_user_id: request.to_user_id}), {method: :patch})%>
      <%=content_tag :td, link_to("Reject", reject_friend_request_path({id:0, from_user_id: request.from_user_id, to_user_id: request.to_user_id}), {method: :patch})%>
    <%end%>
  <%end%>
<%end%>
