<%=content_tag :table do%>
  <%@users.each do |user|%>
    <%=content_tag :tr do%>
      <%=content_tag :td, link_to(user.UserID, user)%>
      <%=content_tag :td, user.Nickname%>
      <%=content_tag :td, user.GradYear%>
      <%=content_tag :td, user.Major%>
      <%if session[:cas_user] == user.UserID%>
        <%=content_tag :td, link_to("edit", edit_user_path(user))%>
      <%end%>
    <%end%>
  <%end%>
<%end%>
